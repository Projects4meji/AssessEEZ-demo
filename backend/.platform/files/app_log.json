{
    "logs": {
        "logs_collected": {
            "files": {
                "collect_list": [
                    {
                        "file_path": "/var/app/current/storage/logs/*.log",
                        "log_group_name": "/aws/elasticbeanstalk/app",
                        "log_stream_name": "{instance_id}_current"
                    },
                    {
                        "file_path": "/var/app/staging/storage/logs/*.log",
                        "log_group_name": "/aws/elasticbeanstalk/app",
                        "log_stream_name": "{instance_id}_staging"
                    }
                ]
            }
        }
    }
}